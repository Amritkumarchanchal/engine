  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "outDir": "./public", // Set the output directory to "public"
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"]
}
